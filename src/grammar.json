{
  "name": "poweron",
  "rules": {
    "program": {
      "type": "REPEAT",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "comment"
          },
          {
            "type": "SYMBOL",
            "name": "multiline_comment"
          },
          {
            "type": "SYMBOL",
            "name": "define_block"
          },
          {
            "type": "SYMBOL",
            "name": "target_block"
          }
        ]
      }
    },
    "multiline_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "["
          },
          {
            "type": "PATTERN",
            "value": "[^\\]]*"
          },
          {
            "type": "STRING",
            "value": "]"
          }
        ]
      }
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Z|a-z]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "data_type": {
      "type": "PATTERN",
      "value": "NUMBER|CHARACTER|[0-9]+"
    },
    "endvar": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "varset": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "SYMBOL",
              "name": "identifier"
            }
          ]
        }
      ]
    },
    "define_var": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "data_type"
        }
      ]
    },
    "define_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "DEFINE"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "define_var"
              },
              {
                "type": "SYMBOL",
                "name": "comment"
              },
              {
                "type": "SYMBOL",
                "name": "multiline_comment"
              }
            ]
          }
        },
        {
          "type": "STRING",
          "value": "END"
        }
      ]
    },
    "target_block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "TARGET"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

